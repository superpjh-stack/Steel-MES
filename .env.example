# Metal-MES Environment Variables
# Copy this file to .env.local and fill in values

# PostgreSQL connection string (로컬: docker-compose 기준)
DATABASE_URL="postgresql://mes_user:mes_secure_pw@localhost:5432/mes_db"

# NextAuth configuration
# Generate secret: openssl rand -base64 32
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="your-secret-at-least-32-characters-long-here"

# Node environment
NODE_ENV="development"
